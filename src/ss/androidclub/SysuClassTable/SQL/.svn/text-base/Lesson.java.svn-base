package ss.androidclub.SysuClassTable.SQL;

public class Lesson {
	private String time;
	private String name;
	private String classroom;
	private String weeks;
	private String danShuangZhou;
	private String weekPosition;
	
	public Lesson() {
		
	}
	
	public Lesson(String kcmc, String jc, String dd, String zfw, String dsz, String weekpos) {
		name = kcmc;
		time = jc;
		classroom = dd;
		weeks = zfw;
		danShuangZhou = dsz;
		weekPosition = weekpos;
	}
	
	public void setTime(String jc) {
		time = jc;
	}
	
	public void setName(String kcmc) {
		name = kcmc;
	}
	
	public void setClassRoom(String dd) {
		classroom = dd;
	}
	
	public void setWeeks(String zfw) {
		weeks = zfw;
	}
	
	public void setDanShuangZhou(String dsz) {
		danShuangZhou = dsz;
	}
	
	public void setWeekPosition(String weekpos) {
		weekPosition = weekpos;
	}
	
	public String getTime() {
		return time;
	}
	
	public String getName() {
		return name;
	}
	
	public String getClassRoom() {
		return classroom;
	}
	
	public String getWeeks() {
		return weeks;
	}
	
	public String getDanShuangZhou() {
		return danShuangZhou;
	}
	
	public String getWeekPosition() {
		return weekPosition;
	}
	
	@Override
	public String toString() {
		String str = "";
		
		str = time + "," + name + "," + classroom + "," + weeks + "," + danShuangZhou + "," + weekPosition;
		
		return str;
	}

	
}